{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "env": {
    "VITE_SUPABASE_URL": {
      "description": "Your Supabase project URL",
      "required": true
    },
    "VITE_SUPABASE_ANON_KEY": {
      "description": "Your Supabase anonymous/public key",
      "required": true
    }
  }
}
